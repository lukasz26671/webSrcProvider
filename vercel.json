{
    "version": 2,
    "builds": [
        {"src": "*.js", "use": "@vercel/node"},
        {"src": "*.ts", "use": "@vercel/node"}
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "api/readplaylist": {},
        "api/readplaylist/featured": {},
        "api/visualized/readplaylist": {},
        "api/visualized/readplaylist/featured": {},
        "/": {}
    }
}